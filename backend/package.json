{
  "name": "serbian-auto-parts-backend",
  "version": "1.0.0",
  "description": "Backend for Serbian Auto Parts E-Commerce Platform using MedusaJS",
  "main": "src/index.js",
  "scripts": {
    "build": "babel src -d dist --extensions \".ts,.js\"",
    "start": "medusa start",
    "dev": "medusa develop",
    "test": "jest --passWithNoTests",
    "seed": "medusa seed -f ./data/seed.json",
    "prepare": "husky install"
  },
  "dependencies": {
    "@medusajs/medusa": "^1.8.0",
    "@medusajs/medusa-cli": "^1.3.7",
    "@medusajs/medusa-fulfillment-manual": "^1.1.44",
    "@medusajs/medusa-payment-manual": "^1.0.19",
    "@medusajs/file-local": "^1.0.0",
    "@medusajs/admin": "^1.8.0",
    "medusa-interfaces": "^1.3.7",
    "typeorm": "^0.3.11",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "redis": "^4.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.19.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.2",
    "@babel/plugin-transform-runtime": "^7.19.1",
    "@babel/preset-env": "^7.19.0",
    "@babel/preset-typescript": "^7.18.6",
    "@types/node": "^18.7.18",
    "babel-jest": "^29.0.3",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "husky": "^8.0.1",
    "jest": "^29.0.3",
    "typescript": "^4.8.3"
  },
  "engines": {
    "node": "^16.20.0 || ^18.0.0"
  },
  "keywords": [
    "ecommerce",
    "auto parts",
    "medusajs",
    "nodejs"
